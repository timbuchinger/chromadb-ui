  "devDependencies": {
    "@types/cypress": "^0.1.6",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.13",
    "cypress": "^14.0.3",
    "markdownlint-cli": "^0.39.0",
    "postcss": "^8.4.21",
    "start-server-and-test": "^2.0.10",
    "tailwindcss": "^3.2.4",
